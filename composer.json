{
    "name": "we-simply-code/laravel-afas-rest-connector",
    "description": "This package integrates the AFAS REST API with Laravel",
    "type": "library",
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sunil Kisoensingh",
            "email": "sunil.kisoensingh@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WeSimplyCode\\LaravelAfasRestConnector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeSimplyCode\\LaravelAfasRestConnector\\LaravelAfasRestConnectorServiceProvider"
            ],
            "aliases": {
                "Afas": "WeSimplyCode\\LaravelAfasRestConnector\\Facades\\Afas"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
